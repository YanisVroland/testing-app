{
    "env": {
        "browser": true,
        "es2021": true
    },
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "plugins": ["react"],
    "extends": [
        "eslint:recommended",
        "eslint-config-prettier",
        "plugin:react/recommended",
        "google"
    ],
    "rules": {
        "strict": ["error", "never"]
    }
}
